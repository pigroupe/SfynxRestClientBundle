{
    "name": "sfynx-project/rest-client-bundle",
    "target-dir": "Sfynx/RestClientBundle",
    "type": "symfony-bundle",
    "description": "SfynxRestClientBundle is a Symfony2's bundle allowing to discuss in a simple and secure way with an API.",
    "keywords": ["api", "client", "bundle", "sfynx", "symfony2", "symfony", "simple"],
    "homepage": "https://github.com/pigroupe/SfynxRestClientBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Prelot",
            "email": "tprelot@gmail.com"
        },
        {
            "name": "Gabriel Bondaz",
            "email": "gabriel.bondaz@gmail.com"
        },
        {
            "name": "Etienne de Longeaux",
            "email": "etienne.delongeaux@gmail.com",
            "homepage": "http://www.pi-groupe.fr",
            "role": "Copyright Holder"
        }
    ],
    "require": {
        "php": ">=5.5",
        "ext-curl": "*",
        "symfony/dependency-injection" : ">=2.1",
        "symfony/config" : ">=2.1",
        "doctrine/doctrine-cache-bundle": "~1.0",
        "sfynx-project/tool-circuitbreaker-bundle": "2.8.2"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-0": { "Sfynx\\RestClientBundle": "" }
    },

    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    },

    "minimum-stability": "dev"
}
